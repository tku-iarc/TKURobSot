<?xml version="1.0" ?>
<launch> 
  <group ns="$(env ROBOT_NS)">
    <node name="Attack_motion" pkg="motion" type="fira6"/>

    <!--camera-->
    <node name="prosilica_driver" pkg="prosilica_camera" type="prosilica_node" respawn="false" respawn_delay="5" output="screen">
      <rosparam command="load" file="$(find vision)/config/prosilica_driver.yaml"/>
    </node>

    <include file="$(find strategy)/launch/vision.launch" />
    <include file="$(find strategy)/launch/localization.launch" />
  </group>
</launch>
